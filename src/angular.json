{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "onigiri": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        }
      },
      "root": "projects/onigiri",
      "sourceRoot": "projects/onigiri/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/onigiri",
            "index": "projects/onigiri/src/index.html",
            "browser": "projects/onigiri/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/onigiri/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/onigiri/src/staticwebapp.config.json",
              "projects/onigiri/src/onigiri_logo.png",
              "projects/onigiri/src/assets",
              "projects/onigiri/src/_redirects",
              {
                "glob": "**/*",
                "input": "./projects/oni-shared/src/assets",
                "output": "/assets/oni-shared/"
              }
            ],
            "styles": [
              "node_modules/flag-icons/css/flag-icons.min.css",
              "node_modules/primeng/resources/themes/mdc-light-indigo/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/primeicons/primeicons.css",
              "projects/onigiri/src/styles/styles.scss"
            ],
            "outputHashing": "media",
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "35kb",
                  "maximumError": "50kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/onigiri/src/environments/environment.ts",
                  "with": "projects/onigiri/src/environments/environment.production.ts"
                }
              ],
              "scripts": [
                "projects/onigiri/src/clarity.js",
                "projects/onigiri/src/crisp.js"
              ]
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "35kb",
                  "maximumError": "50kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/onigiri/src/environments/environment.ts",
                  "with": "projects/onigiri/src/environments/environment.staging.ts"
                }
              ],
              "scripts": []
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "onigiri:build:production"
            },
            "development": {
              "buildTarget": "onigiri:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "onigiri:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/onigiri/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/onigiri/src/favicon.ico",
              "projects/onigiri/src/assets"
            ],
            "styles": [
              "projects/onigiri/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
